 return ChatResponse(
            role=response.get("role", "assistant"),
            content=response.get("content", ""),
            summary=response.get("summary", ""),
            query=response.get("query", ""),
            visualizationApplicable=response.get("visualizationApplicable", False),
            appropriateVisualizations=response.get("appropriateVisualizations", []),
            chartResponses=response.get("chartResponses", []),
            results=response.get("results", []),
            timeTaken=response.get("timeTaken", None)
        )
